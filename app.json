{
  "expo": {
    "name": "Sport On",
    "slug": "sportsOn",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/inter/Inter_100Thin.ttf"
          ]
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(SPORT ON) to use your location."
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "purposeStrings": {
        "NSLocationAlwaysUsageDescription": "Darawal helps mechanics find nearby jobs and clients. Allow continuous location access for efficient job matching.",
        "NSLocationWhenInUseUsageDescription": "Darawal shows relevant jobs near your location. Allow location access only while actively searching for jobs."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "package": "com.spondias.sportsOn",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCYS3v8KzdkJ47tevkzLUnMxXODbgeaWsk"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "9c3fc314-66a1-4fb3-aabf-e7f7777e1f91"
      }
    }
  }
}
